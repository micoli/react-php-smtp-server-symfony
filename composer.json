{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-iconv": "*",
        "ext-openssl": "*",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.4.2",
        "doctrine/doctrine-migrations-bundle": "^3.1.1",
        "doctrine/orm": "^2.9",
        "fzaninotto/faker": "1.9.x-dev",
        "micoli/react-php-smtp-server": "dev-master",
        "nyholm/psr7": "^1.1",
        "overblog/graphql-bundle": "^0.13.6",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "5.2.*",
        "symfony/event-dispatcher": "5.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-foundation": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/security-bundle": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/uid": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "5.2.*",
        "zbateson/mail-mime-parser": "^1.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4",
        "php-coveralls/php-coveralls": "dev-master",
        "phpstan/phpstan": "^0.12.90",
        "phpunit/phpunit": "^9.5",
        "seregazhuk/php-watcher": "dev-master",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/phpunit-bridge": "5.2.*"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/micoli/react-php-smtp-server"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "extra": {
            "symfony": {
                "allow-contrib": true,
                "require": "5.2.*"
            }
        }
    }
}
